    "multer": "1.3.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "6.0.1",
    "preact": "8.2.7",
    "preact-markup": "1.6.0",
    "preact-portal": "1.1.2",
    "preact-router": "2.6.0",
    "promise-polyfill": "6.1.0",
    "proptypes": "1.1.0",
